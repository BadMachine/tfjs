{
  "name": "@tensorflow/tfjs-tflite",
  "version": "0.0.1",
  "description": "TFLite support for TensorFlow.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/tensorflow/tfjs.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "yarn": ">= 1.3.2"
  },
  "peerDependencies": {
    "@tensorflow/tfjs-core": "link:../tfjs-core"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-typescript": "^3.0.0",
    "@tensorflow/tfjs-core": "link:../tfjs-core",
    "@types/jasmine": "~2.8.6",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-polyfill": "~6.26.0",
    "babel-preset-env": "~1.6.1",
    "clang-format": "~1.2.2",
    "jasmine": "~3.1.0",
    "parcel-bundler": "^1.12.3",
    "rimraf": "~2.6.2",
    "rollup": "~2.3.2",
    "rollup-plugin-terser": "~5.3.0",
    "rollup-plugin-visualizer": "~3.3.2",
    "tmp": "^0.0.33",
    "ts-node": "^5.0.1",
    "tslint": "~5.9.1",
    "tslint-no-circular-imports": "^0.7.0",
    "typescript": "3.5.3",
    "yalc": "~1.0.0-pre.21"
  },
  "scripts": {
    "build": "tsc",
    "build-ci": "tsc && mkdir -p dist/proto && cp src/proto/api_pb.js dist/proto/api_pb.js",
    "build-core": "cd ../tfjs-core && yarn && yarn build",
    "build-core-ci": "cd ../tfjs-core && yarn && yarn build-ci",
    "build-deps": "yarn build-core",
    "build-deps-ci": "yarn build-core-ci",
    "build-npm": "./scripts/build-npm.sh",
    "lint": "tslint -p . -t verbose",
    "publish-npm": "npm publish",
    "test": "yarn && yarn build-deps && yarn build && ts-node --transpile-only -P tsconfig.test.json src/run_tests.ts",
    "test-ci": "ts-node --transpile-only --skip-ignore -P tsconfig.test.json src/run_tests.ts"
  }
}
